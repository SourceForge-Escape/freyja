(setq MAIN_WINDOW (window "Freyja" "freyja.png" 

	;; Window container
	(vbox 0 0 0 1 0

		;; Menubar
		(menubar
			(submenu "_File" 
				(menu_item "_New"			"C-n"	1	"gtk-new")
				(menu_item "_Open..."		"C-o"	2	"gtk-open")
				(menu_item "Ap_pend..."  			3	"gtk-paste")
				(menu_separator)
				(menu_item "_Quit"						"gtk-quit")
			)

			(submenu "_Edit" 
				(menu_item "C_ut"			"C-x"	11	"gtk-cut")
				(menu_item "_Copy"			"C-c"	12	"gtk-copy")
				(menu_item "_Paste"  				13	"gtk-paste")
			)
		)
		
		(handlebox 1 
			(hbox 1 1 0 0 0
				(button "_New" eMode eNewFile)
				(button "_Open" eMode eOpenFile)
				(button "_Save" eMode eSaveFile)
				(button "_Quit" eMode eQuitFile) ) )

	) ;; vbox
	
